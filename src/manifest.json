{
  "manifest_version": 3,
  "name": "Link Extractor",
  "version": "2.6.1",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://sites.google.com/site/linkgopher/",

  "icons": {
    "16": "assets/logo-black-16.png",
    "48": "assets/logo-black-48.png",
    "128": "assets/logo-black-128.png"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "link-extractor@cssnr.com",
      "strict_min_version": "109.0"
    }
  },

  "host_permissions":[
    "*://*/*"
  ],

  "permissions": [
    "scripting"
  ],

  "action": {
    "default_icon": "assets/logo-black-128.png",
    "default_title": "Link Extractor",
    "default_popup": "html/popup.html"
  },

  "content_security_policy": {}
}
